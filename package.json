{
  "name": "file-browser",
  "version": "0.0.1",
  "author": "Abachouch Youssouf",
  "description": "minimal yet just what you need file explorer",
  "main": "mainProcess/electron.js",
  "homepage": "./",
  "license": "AGPL-3.0-or-later",
  "icon": "./statics/icon512.png",
  "scripts": {
    "start": "electron .",
    "dev": "webpack --config ./webpack.config.js --env MODE=development",
    "build": "webpack --config ./webpack.config.js --env MODE=production",
    "pack": "electron-builder --dir",
    "dist": "electron-builder build",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint src/",
    "style": "prettier -w src/"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.1.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "copy-webpack-plugin": "^9.0.1",
    "music-metadata": "^7.11.0",
    "nedb-promises": "^5.0.0",
    "node-disk-info": "^1.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4"
  },
  "build": {
    "appId": "io.github.abachouch.file-browser",
    "productName": "file-browser",
    "copyright": "Copyright Â© 2021 Abachouch Youssouf",
    "icon": "./build/icon512.png",
    "files": [
      "build/**/*",
      "mainProcess/**/*",
      "statics/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
